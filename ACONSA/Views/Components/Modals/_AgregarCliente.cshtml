<div class="modal fade" id="addCliente" tabindex="-1" role="dialog" aria-labelledby="smallmodalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title" id="smallmodalLabel">Nuevo Cliente</h3>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form class="container">
                    <div class="row">
                        <div class="col-12">
                            <h4>Datos generales </h4>
                            <hr />
                        </div>
                        <div class="form-group col-2">
                            <label>Código: </label>
                            <input type="text" class="form-control" id="" disabled placeholder="#-###">
                        </div>
                        <div class="form-group col-3">
                            <label>Tipo de Cliente: </label>
                            <input name="" type="text" class="form-control" data-val="true"
                                   autocomplete="off"
                                   list="ListaTipoCliente">
                            <datalist id="ListaTipoCliente">
                                <option>Persona natural</option>
                                <option>Empresa | Persona Jurídica</option>
                            </datalist>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-12">
                            <label for="">Nombres: </label>
                            <input type="text" class="form-control" id="" placeholder="">
                        </div>
                        <div class="form-group col-12">
                            <label for="">Apellidos: </label>
                            <input type="text" class="form-control" id="" placeholder="">
                        </div>
                        <div class="form-group col-3">
                            <label for="">DUI: </label>
                            <input type="number" class="form-control" id="" placeholder="">
                        </div>
                        <div class="form-group col-3">
                            <label for="">NIT: </label>
                            <input type="number" class="form-control" id="" placeholder="">
                        </div>
                    
                        <div class="col-12">
                            <h4>Contacto </h4>
                            <hr />
                        </div>
                        <div class="form-group col-4">
                            <label for="">Teléfono: </label>
                            <input type="number" class="form-control" id="" placeholder="">
                        </div>
                        <div class="form-group col-8">
                            <label for="">Correo electrónico: </label>
                            <input type="text" class="form-control" id="" placeholder="">
                        </div>
                        <div class="form-group col-3">
                            <label for="">Departamento: </label>
                            <input name="" type="text" class="form-control" data-val="true"
                                   autocomplete="off"
                                   list="ListaDeptos">
                            <datalist id="ListaDeptos">
                                <option>Depto 1</option>
                                <option>Depto 2</option>
                                <option>Depto 3</option>
                                <option>Depto 4</option>
                                <option>Depto 5</option>
                                <option>Depto 6</option>
                                <option>Depto 7</option>
                                <option>Depto 8</option>
                                <option>Depto 9</option>
                                <option>Depto 10</option>
                                <option>Depto 11</option>
                                <option>Depto 12</option>
                                <option>Depto 13</option>
                                <option>Depto 14</option>
                            </datalist>
                        </div>
                        <div class="form-group col-3">
                            <label for="">Municipio: </label>
                            <input name="" type="text" class="form-control" data-val="true"
                                   autocomplete="off"
                                   list="ListaMunicipios">
                            <datalist id="ListaMunicipios">
                                <option>Municipio 1</option>
                                <option>Municipio 3</option>
                            </datalist>
                        </div>
                        <div class="form-group col-6">
                            <label for="">Dirección: </label>
                            <input type="text" class="form-control" id="" placeholder="">
                        </div>
                    </div>
                    <div class="row" id="negocioDataSet">
                        <div class="col-12">
                            <h4>Negocio: </h4>
                            <hr />
                        </div>
                        <div class="form-group col-4">
                            <label for="">NRC: </label>
                            <input type="number" class="form-control" id="" placeholder="">
                        </div>
                        <div class="form-group col-4">
                            <label for="">Giro: </label>
                            <input type="text" class="form-control" id="" placeholder="">
                        </div>
                    </div>
</form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                <button type="button" class="btn btn-primary">Agregar</button>
            </div>
        </div>
    </div>
</div>